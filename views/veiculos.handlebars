<h1>Veiculos Cadastrados</h1>

<table border="1">

    <tr>
    <td>ID</td>
    <td>Modelo</td>
    <td>Placa</td>
    <td>Cor</td>
    <td>ano</td>
</tr>

{{#each veiculos}}
<tr>
    <td>{{this.id}}</td>
    <td>{{this.modelo}}</td>
    <td>{{this.placa}}</td>
    <td>{{this.cor}}</td>
    <td>{{this.ano}}</td>
</tr>


{{/each}}
</table>
